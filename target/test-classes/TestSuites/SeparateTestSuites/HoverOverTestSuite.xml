<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="KeyPressTests" verbose="1">

    <test name="KeyPressTest - CH">
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="com.herokuapp.theinternet.hovertests.HoverTests" />
        </classes>
    </test>

    <test name="KeyPressTest - FF">
        <parameter name="browser" value="firefox"></parameter>
        <classes>
            <class name="com.herokuapp.theinternet.hovertests.HoverTests" />
        </classes>
    </test>

</suite>